telemetry.postcapitalist.io {
  reverse_proxy 127.0.0.1:3000
  route {
    reverse_proxy /feed 127.0.0.1:8000
    header Connection *Upgrade*
    header Upgrade    websocket
  }
  route {
    reverse_proxy /submit 127.0.0.1:8001
    header Connection *Upgrade*
    header Upgrade    websocket
  }
  log {
    output stdout
  }
}
